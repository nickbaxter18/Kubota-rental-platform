{
  "name": "udigit-rentals-backend",
  "version": "1.0.0",
  "description": "U-Dig It Rentals - Kubota SVL-75 Rental Platform Backend API",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:critical": "jest --testPathPattern=critical --verbose",
    "test:integration": "jest --testPathPattern=integration --verbose --setupFilesAfterEnv=./src/test/integration-setup.ts",
    "typeorm": "typeorm",
    "migration:generate": "npx tsx ./typeorm.config.ts migration:generate",
    "migration:create": "npx tsx ./typeorm.config.ts migration:create",
    "migration:run": "npx tsx ./typeorm.config.ts migration:run",
    "migration:revert": "npx tsx ./node_modules/typeorm/cli.js -d typeorm.config.ts migration:revert",
    "seed": "npx tsx ./node_modules/typeorm/cli.js -d typeorm.config.ts migration:run",
    "clean": "rimraf dist node_modules/.cache",
    "prebuild": "pnpm clean"
  },
  "keywords": [
    "nestjs",
    "rental",
    "kubota",
    "equipment",
    "booking",
    "stripe",
    "docusign"
  ],
  "author": "U-Dig It Rentals Inc.",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.2.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sentry/node": "^10.17.0",
    "@stripe/stripe-js": "^8.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/passport-jwt": "^4.0.1",
